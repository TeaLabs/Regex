{
    "name": "tea/regex",
    "description": "A clean interface for building and working with regular expressions. Lightweight wrapper for the built in PCRE library (preg_* functions).",
    "keywords": [
        "regex", "expressions", "regular", "expressions", "preg", "PCRE", "utils", "tea"
    ],
    "license": "MIT",
    "homepage": "https://github.com/TeaLabs/regex",
    "support": {
        "issues": "https://github.com/TeaLabs/regex/issues",
        "source": "https://github.com/TeaLabs/regex"
    },
    "authors": [
        {
            "name": "David Kyalo",
            "email": "davidmkyalo@gmail.com",
            "homepage": "http://www.davidkyalo.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "tea/future": "~0.1.0",
        "tea/contracts": "~0.1.0",
        "gherkins/regexpbuilderphp" : "0.*.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "phpbench/phpbench": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Tea\\Regex\\": "src/"
        },
        "files" : [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tea\\Regex\\Tests\\": "tests/"
        },
        "files" : [
            "tests/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
